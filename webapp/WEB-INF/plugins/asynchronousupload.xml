<?xml version="1.0" encoding="UTF-8"?><plug-in>

    <!-- Plugin Informations -->
    <name>asynchronous-upload</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.1.11-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>asynchronousupload.plugin.description</description>
    <provider>asynchronousupload.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/asynchronousupload/asynchronousupload.png</icon-url>
    <copyright>Copyright 2001-2014 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>4.4.1</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>
    
    <css-stylesheets>
        <css-stylesheets-scope>portal</css-stylesheets-scope>
        <css-stylesheet>asynchronousupload/jquery.fileupload.css</css-stylesheet>
        <css-stylesheet>asynchronousupload/jquery.fileupload-ui.css</css-stylesheet>
    </css-stylesheets>
    
	<javascript-files>
        <javascript-files-scope>portal</javascript-files-scope>
        <!-- vendor -->
        <javascript-file>asynchronousupload/vendor/jquery.ui.widget.js</javascript-file>
        <javascript-file>asynchronousupload/vendor/canvas-to-blob.js</javascript-file>
        <javascript-file>asynchronousupload/vendor/jquery.Jcrop.js</javascript-file>
        <javascript-file>asynchronousupload/vendor/promise-polyfill.js</javascript-file>
        <!-- base -->
        <javascript-file>asynchronousupload/jquery.iframe-transport.js</javascript-file>
        <javascript-file>asynchronousupload/load-image.all.min.js</javascript-file> 
        <javascript-file>asynchronousupload/jquery.fileupload.js</javascript-file>
        <javascript-file>asynchronousupload/jquery.fileupload-process.js</javascript-file>
        <javascript-file>asynchronousupload/jquery.fileupload-image.js</javascript-file>
        <javascript-file>asynchronousupload/jquery.fileupload-validate.js</javascript-file>
		<javascript-file>asynchronousupload/config.js</javascript-file>
    </javascript-files>

    <css-admin-stylesheets>
        <css-admin-stylesheet>css/plugins/asynchronousupload/jquery.fileupload.css</css-admin-stylesheet>
        <css-admin-stylesheet>css/plugins/asynchronousupload/jquery.fileupload-ui.css</css-admin-stylesheet>
    </css-admin-stylesheets>

    <admin-javascript-files>      
        <!-- vendor -->
        <admin-javascript-file>js/plugins/asynchronousupload/vendor/jquery.ui.widget.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/vendor/canvas-to-blob.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/vendor/jquery.Jcrop.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/vendor/promise-polyfill.js</admin-javascript-file>
        <!-- base -->
        <admin-javascript-file>js/plugins/asynchronousupload/jquery.iframe-transport.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/load-image.all.min.js</admin-javascript-file> 
        <admin-javascript-file>js/plugins/asynchronousupload/jquery.fileupload.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/jquery.fileupload-process.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/jquery.fileupload-image.js</admin-javascript-file>
        <admin-javascript-file>js/plugins/asynchronousupload/jquery.fileupload-validate.js</admin-javascript-file>
		<admin-javascript-file>js/admin/plugins/asynchronousupload/config.js</admin-javascript-file>
    </admin-javascript-files>   

	<listeners>
	   <listener>
	       <listener-class>fr.paris.lutece.plugins.asynchronousupload.service.AsynchronousUploadSessionListener</listener-class>
	   </listener>
	</listeners>
</plug-in>